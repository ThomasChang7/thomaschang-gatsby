{
  "extends":[
    "prettier",
    "prettier/react",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins":[
   "prettier",
   "react",
   "jsx-a11y",
   "import"
  ],
  "parserOptions":{
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}